<?xml version="1.0" encoding="UTF-8" ?>
<items>
    {% for page in site.pages %}
        {% if page.path contains 'item/' %}
            <item>
                <title>{{ page.title }}</title>
                <url>{{ site.url }}{{ site.baseurl }}{{ page.url }}</url>
                <tags>
                    {% for tag in page.tags %}
                        <tag>{{ tag }}</tag>
                    {% endfor %}
                </tags>
            </item>
        {% endif %}
    {% endfor %}
</items>